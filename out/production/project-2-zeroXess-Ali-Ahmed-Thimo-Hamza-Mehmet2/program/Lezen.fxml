<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="program.LezenController">
   <children>
      <TextArea layoutX="2.0" prefHeight="105.0" prefWidth="800.0" text="A mother chicken sat on her nest. She was a chicken with white feathers. She was sitting on some eggs. One day the eggs cracked open." wrapText="true">
         <font>
            <Font size="21.0" />
         </font></TextArea>

      <Button fx:id="submitButton" layoutX="374.0" layoutY="457.0" mnemonicParsing="false" onAction="#submit" text="Submit" />

      <Label layoutX="243.0" layoutY="105.0" text="Read the story and answer the question">
         <font>
            <Font size="18.0" />
         </font></Label>
      <TextArea id="QuestionField" fx:id="QuestionField" layoutX="48.0" layoutY="144.0" prefHeight="200.0" prefWidth="706.0" />
      <TextField id="AnswerField" fx:id="AnswerField" layoutX="327.0" layoutY="425.0" />
      <Button fx:id="startButton" layoutX="363.0" layoutY="367.0" mnemonicParsing="false" onAction="#startTest" text="Start Test">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="submitButton2" layoutX="734.0" layoutY="561.0" mnemonicParsing="false" onAction="#toHomescreen" text="Home" />
      <Label fx:id="Feedback" layoutX="387.0" layoutY="397.0" />

   </children>
</AnchorPane>
